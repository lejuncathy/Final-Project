# Given constants
log_8 <- log(8)
z_critical <- qnorm(0.2)  # z_critical = Î¦^(-1)(0.2)
# Function to solve for n
solve_n <- function() {
uniroot(function(n) {
left <- log_8 + n / 2 - n
right <- z_critical * sqrt(n)
left - right
}, lower = 1, upper = 1000)$root
}
